<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="condominio.fx.FXMLEntradaController">
   <children>
      <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label text="Casa:">
               <VBox.margin>
                  <Insets bottom="15.0" left="10.0" />
               </VBox.margin>
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Label>
            <TextField fx:id="tfCasa" maxWidth="-Infinity" prefHeight="25.0" prefWidth="60.0">
               <VBox.margin>
                  <Insets bottom="15.0" left="10.0" />
               </VBox.margin>
               <font>
                  <Font size="14.0" />
               </font>
            </TextField>
            <Label text="Nome:">
               <VBox.margin>
                  <Insets bottom="15.0" left="10.0" />
               </VBox.margin>
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Label>
            <TextField fx:id="tfNome" promptText="nome do visitante ">
               <VBox.margin>
                  <Insets bottom="15.0" left="10.0" right="10.0" />
               </VBox.margin>
               <font>
                  <Font size="14.0" />
               </font>
            </TextField>
            <Label text="Placa:">
               <VBox.margin>
                  <Insets bottom="15.0" left="10.0" />
               </VBox.margin>
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Label>
            <TextField fx:id="tfPlaca" cacheShape="false" centerShape="false" focusTraversable="false" maxWidth="-Infinity" prefHeight="30.0" prefWidth="100.0" scaleShape="false">
               <VBox.margin>
                  <Insets bottom="15.0" left="10.0" />
               </VBox.margin>
               <font>
                  <Font size="14.0" />
               </font>
            </TextField>
            <Label text="Entrada:">
               <VBox.margin>
                  <Insets bottom="15.0" left="10.0" />
               </VBox.margin>
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Label>
            <TextField fx:id="tfEntrada" maxWidth="-Infinity" prefHeight="30.0" prefWidth="100.0">
               <VBox.margin>
                  <Insets bottom="15.0" left="10.0" />
               </VBox.margin>
               <font>
                  <Font size="14.0" />
               </font>
            </TextField>
            <HBox prefHeight="22.0" prefWidth="600.0">
               <children>
                  <CheckBox fx:id="cbUber" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" text="Uber / 99 / Taxi" HBox.hgrow="NEVER">
                     <font>
                        <Font name="System Bold" size="14.0" />
                     </font>
                     <HBox.margin>
                        <Insets right="15.0" />
                     </HBox.margin>
                  </CheckBox>
                  <TextField fx:id="tfMotorista" promptText="nome do motorista" HBox.hgrow="SOMETIMES">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </TextField>
               </children>
               <VBox.margin>
                  <Insets bottom="15.0" left="10.0" right="10.0" />
               </VBox.margin>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <VBox.margin>
                  <Insets left="10.0" right="10.0" />
               </VBox.margin>
               <children>
                  <Button fx:id="btnCancelar" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#botaoCancelar" text="cancelar">
                     <HBox.margin>
                        <Insets right="15.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="btnOk" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#botaoOk" text="ok" />
                  <Label fx:id="ID" text="ID" visible="false">
                     <HBox.margin>
                        <Insets left="20.0" />
                     </HBox.margin>
                  </Label>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
